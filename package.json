{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "10.x",
    "d3": "^5.15.0",
    "d3-array": "^2.4.0",
    "gatsby": "^2.20.35",
    "gatsby-image": "^2.3.5",
    "gatsby-plugin-manifest": "^2.3.7",
    "gatsby-plugin-offline": "^3.1.5",
    "gatsby-plugin-postcss": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.2.5",
    "gatsby-plugin-sharp": "^2.5.7",
    "gatsby-plugin-typescript": "^2.4.0",
    "gatsby-plugin-typography": "^2.4.4",
    "gatsby-source-filesystem": "^2.2.5",
    "gatsby-transformer-json": "^2.3.4",
    "gatsby-transformer-sharp": "^2.4.7",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-lincoln": "^0.16.19"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "prettier": "2.0.4",
    "tailwindcss": "^1.3.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:tailwind": "postcss tailwind.css -o tailwind.generated.css",
    "watch:tailwind": "postcss -w tailwind.css -o tailwind.generated.css",
    "start": "run-p watch:tailwind develop",
    "develop": "gatsby develop",
    "prebuild": "yarn build:tailwind",
    "build": "gatsby build",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
